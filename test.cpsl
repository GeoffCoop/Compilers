var
  a : integer; b : boolean; c : char;
begin
	a := 1+7;
	write(a);
	a:= succ(a);
	write(a);
	a:= pred(a);
	write(a);
	a:=-a;
	write(a);
	a := 3-1;
	write(a);
	a := 4*4;
	write(a);
	a := 3/2;
	write(a);
	a := 33%34;
	write(a);
	
	c := chr(a);
	write(c);
	write(ord(c));

	b:= true;
	write(b);
	b:= false;
	write(b);
	b := true&true;
	write(b);
	b := true|true;
	b := 1<=1;
	b := 1>=1;
	b := 1<>1;
	b := 1<1;
	b := 1>1;
	b := 1=1;
end.
